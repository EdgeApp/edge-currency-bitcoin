{
  "name": "edge-currency-bitcoin",
  "version": "4.9.16",
  "description": "Edge Bitcoin currency plugin",
  "homepage": "https://edge.app",
  "repository": {
    "type": "git",
    "url": "git@github.com:Airbitz/edge-currency-bitcoin.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Edge",
  "contributors": [
    "Eliran Zach <eliran@edge.app>",
    "William Swanson <william@edge.app>",
    "Paul Puey <paul@edge.app>",
    "Stan Synko <stan@airbitz.co>",
    "Kylan Hurt <kylan@edge.app>"
  ],
  "files": [
    "CHANGELOG.md",
    "index.js",
    "lib/*",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "main": "./index.js",
  "module": "./lib/index.js",
  "scripts": {
    "build": "rimraf lib && sucrase ./src -q -d ./lib -t flow && webpack",
    "fix": "npm run lint -- --fix",
    "flow": "flow",
    "lint": "eslint .",
    "precommit": "lint-staged && flow && npm test && npm run build",
    "prepare": "npm run build",
    "security": "yarn audit",
    "test": "nyc mocha -r sucrase/register './test/plugin/currencyPlugin/currencyPlugin.js'",
    "posttest": "nyc report --reporter=cobertura --reporter=html",
    "updot": "updot"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "dependencies": {
    "bcoin": "git+https://github.com/Airbitz/bcoin.git#primitiveBuild",
    "biggystring": "3.0.2",
    "bn.js": "^4.11.8",
    "bs58grscheck": "https://github.com/Groestlcoin/bs58grscheck",
    "bs58smartcheck": "^2.0.4",
    "buffer": "^5.2.1",
    "chai": "^4.3.4",
    "cleaners": "^0.3.9",
    "esm": "^3.2.25",
    "eventemitter3": "^3.1.0",
    "jsonschema": "^1.4.0",
    "secp256k1": "^3.7.1",
    "stable": "^0.1.8",
    "unorm": "^1.6.0",
    "uri-js": "^4.4.1",
    "url-parse": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.5",
    "crypto-browserify": "^3.12.0",
    "edge-core-js": "^0.17.33",
    "eslint": "^7.14.0",
    "eslint-config-standard-kit": "0.15.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "flow-bin": "^0.82.0",
    "husky": "^4.3.0",
    "jsonfile": "^5.0.0",
    "lint-staged": "^10.5.3",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-multi-reporters": "^1.1.7",
    "mochawesome": "^3.0.3",
    "node-fetch": "^2.2.0",
    "nyc": "^13.0.1",
    "prettier": "^2.2.0",
    "prettier-eslint-cli": "^4.7.1",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "stream": "^0.0.2",
    "sucrase": "^3.9.5",
    "updot": "^1.1.7",
    "url": "^0.11.0",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "react-native": "./lib/index.js"
}
